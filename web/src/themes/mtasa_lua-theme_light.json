{
  "name": "mta-highlight-light",
  "type": "light",
  "colors": {
    "editor.foreground": "#333333",
    "editor.background": "#fafafa"
  },

  "tokenColors": [
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#16f756",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lua",
      "settings": {
        "foreground": "#16f756",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.mta-client",
      "settings": {
        "foreground": "#cc3d3d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.mta-server",
      "settings": {
        "foreground": "#db9c42",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.mta-shared",
      "settings": {
        "foreground": "#4191d9",
        "fontStyle": "bold"
      }
    },

    {
      "scope": "keyword.mta",
      "settings": {
        "foreground": "#6edb9a",
        "fontStyle": "bold"
      }
    },

    {
      "scope": "meta.function.lua",
      "settings": {
        "foreground": "#16f756"
      }
    },
    {
      "scope": "punctuation.definition.parameters.begin.lua",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": "punctuation.definition.parameters.finish.lua",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": "meta.parameter.lua",
      "settings": {
        "foreground": "#4a90e2"
      }
    },
    {
      "scope": "variable.parameter.function.lua",
      "settings": {
        "foreground": "#4a90e2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.separator.arguments.lua",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": "entity.name.class.lua",
      "settings": {
        "foreground": "#3a9f8a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.numeric.float.hexadecimal.lua",
        "constant.numeric.integer.hexadecimal.lua",
        "constant.numeric.float.lua",
        "constant.numeric.integer.lua"
      ],
      "settings": {
        "foreground": "#6a8f6b",
        "fontStyle": ""
      }
    },
    {
      "scope": "string.lua",
      "settings": {
        "foreground": "#a67c32"
      }
    },
    {
      "scope": "comment.line.shebang.lua",
      "settings": {
        "foreground": "#4a7a32",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.lua",
      "settings": {
        "foreground": "#4a7a32",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.control.goto.lua",
      "settings": {
        "foreground": "#2f5f9c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.tag.lua",
      "settings": {
        "foreground": "#f01adb"
      }
    },
    {
      "scope": "storage.type.attribute.lua",
      "settings": {
        "foreground": "#3a9f8a"
      }
    },
    {
      "scope": "storage.type.generic.lua",
      "settings": {
        "foreground": "#4a90e2"
      }
    },
    {
      "scope": "keyword.control.lua",
      "settings": {
        "foreground": "#f01adb",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.local.lua",
      "settings": {
        "foreground": "#f01adb",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lua",
      "settings": {
        "foreground": "#a83259",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.language.self.lua",
      "settings": {
        "foreground": "#4a90e2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.tag.lua",
      "settings": {
        "foreground": "#f01adb",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.library.lua",
      "settings": {
        "foreground": "#a67c32"
      }
    },
    {
      "scope": "keyword.operator.lua",
      "settings": {
        "foreground": "#f01adb",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.class.lua",
      "settings": {
        "foreground": "#3a9f8a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.other.attribute.lua",
      "settings": {
        "foreground": "#4a90e2"
      }
    },
    {
      "scope": "variable.other.lua",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": "keyword.operator.lua",
      "settings": {
        "foreground": "#f01adb"
      }
    },
    {
      "scope": "punctuation.whitespace.comment.leading.lua",
      "settings": {
        "foreground": "#4a7a32"
      }
    },
    {
      "scope": "punctuation.whitespace.comment.trailing.lua",
      "settings": {
        "foreground": "#4a7a32"
      }
    },
    {
      "scope": "punctuation.definition.comment.begin.lua",
      "settings": {
        "foreground": "#4a7a20",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.definition.comment.end.lua",
      "settings": {
        "foreground": "#4a7a20",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "comment.block.lua",
      "settings": {
        "foreground": "#4a7a32",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.line.double-dash.lua",
      "settings": {
        "foreground": "#4a7a32"
      }
    },
    {
      "scope": "comment.line.double-dash.documentation.lua",
      "settings": {
        "foreground": "#40712d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "storage.type.annotation.lua",
      "settings": {
        "foreground": "#16f756",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.class.lua",
      "settings": {
        "foreground": "#2f5f9c"
      }
    },
    {
      "scope": "support.type.lua",
      "settings": {
        "foreground": "#4a90e2"
      }
    },
    {
      "scope": "entity.name.variable.lua",
      "settings": {
        "foreground": "#4a90e2"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#6a8f6b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.character.escape.lua",
      "settings": {
        "foreground": "#a37543",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.character.escape.byte.lua",
      "settings": {
        "foreground": "#3f6fc9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.character.escape.unicode.lua",
      "settings": {
        "foreground": "#2f7f8c",
        "fontStyle": "600"
      }
    },
    {
      "scope": "invalid.illegal.character.escape.lua",
      "settings": {
        "foreground": "#a83232",
        "background": "#f3dede",
        "fontStyle": "bold underline wavy"
      }
    },
    {
      "scope": "punctuation.definition.block.tag.ldoc",
      "settings": {
        "foreground": "#945e9c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.class.ldoc",
      "settings": {
        "foreground": "#3f6fc9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.quoted.single.lua",
      "settings": {
        "foreground": "#67994d"
      }
    },
    {
      "scope": "string.quoted.double.lua",
      "settings": {
        "foreground": "#67994d"
      }
    },
    {
      "scope": "string.quoted.other.multiline.lua",
      "settings": {
        "foreground": "#67994d"
      }
    },
    {
      "scope": "punctuation.definition.string.begin.lua",
      "settings": {
        "foreground": "#a37543"
      }
    },
    {
      "scope": "punctuation.definition.string.end.lua",
      "settings": {
        "foreground": "#a37543"
      }
    },
    {
      "scope": "constant.character.escape.lua",
      "settings": {
        "foreground": "#945e9c"
      }
    }
  ]
}
