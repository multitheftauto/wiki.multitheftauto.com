{
  "name": "mta-highlight-light",
  "type": "light",
  "colors": {
    "editor.foreground": "#333333",
    "editor.background": "#DDD"
  },

  "tokenColors": [
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#3f9d87",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lua",
      "settings": {
        "foreground": "#3f9d87",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.mta-client",
      "settings": {
        "foreground": "#cc3d3d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.mta-server",
      "settings": {
        "foreground": "#FFA000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.mta-shared",
      "settings": {
        "foreground": "#4191d9",
        "fontStyle": "bold"
      }
    },

    {
      "scope": "keyword.mta",
      "settings": {
        "foreground": "#0aa9aa",
        "fontStyle": "bold"
      }
    },

    {
      "scope": "meta.function.lua",
      "settings": {
        "foreground": "#3f9d87"
      }
    },
    {
      "scope": "punctuation.definition.parameters.begin.lua",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": "punctuation.definition.parameters.finish.lua",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": "meta.parameter.lua",
      "settings": {
        "foreground": "#4ab3d1"
      }
    },
    {
      "scope": "variable.parameter.function.lua",
      "settings": {
        "foreground": "#4ab3d1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.separator.arguments.lua",
      "settings": {
        "foreground": "#e4b849"
      }
    },
    {
      "scope": "entity.name.class.lua",
      "settings": {
        "foreground": "#334ebb",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.numeric.float.hexadecimal.lua",
        "constant.numeric.integer.hexadecimal.lua",
        "constant.numeric.float.lua",
        "constant.numeric.integer.lua"
      ],
      "settings": {
        "foreground": "#e49f4d",
        "fontStyle": ""
      }
    },
    {
      "scope": "string.lua",
      "settings": {
        "foreground": "#080"
      }
    },
    {
      "scope": "comment.line.shebang.lua",
      "settings": {
        "foreground": "#6f6f6e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.lua",
      "settings": {
        "foreground": "#6f6f6e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.control.goto.lua",
      "settings": {
        "foreground": "#2f5f9c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.tag.lua",
      "settings": {
        "foreground": "#a974bf"
      }
    },
    {
      "scope": "storage.type.attribute.lua",
      "settings": {
        "foreground": "#2610bf"
      }
    },
    {
      "scope": "storage.type.generic.lua",
      "settings": {
        "foreground": "#b912a5"
      }
    },
    {
      "scope": "keyword.control.lua",
      "settings": {
        "foreground": "#a974bf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.local.lua",
      "settings": {
        "foreground": "#a974bf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lua",
      "settings": {
        "foreground": "#c6555f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.language.self.lua",
      "settings": {
        "foreground": "#c6555f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.tag.lua",
      "settings": {
        "foreground": "#a974bf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.library.lua",
      "settings": {
        "foreground": "#a67c32"
      }
    },
    {
      "scope": "entity.other.attribute.lua",
      "settings": {
        "foreground": "#4ab3d1"
      }
    },
    {
      "scope": "variable.other.lua",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": "keyword.operator.lua",
      "settings": {
        "foreground": "#d46f4e"
      }
    },
    {
      "scope": "punctuation.whitespace.comment.leading.lua",
      "settings": {
        "foreground": "#6f6f6e"
      }
    },
    {
      "scope": "punctuation.whitespace.comment.trailing.lua",
      "settings": {
        "foreground": "#6f6f6e"
      }
    },
    {
      "scope": "punctuation.definition.comment.begin.lua",
      "settings": {
        "foreground": "#6f6f6e",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.definition.comment.end.lua",
      "settings": {
        "foreground": "#6f6f6e",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "comment.block.lua",
      "settings": {
        "foreground": "#6f6f6e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.line.double-dash.lua",
      "settings": {
        "foreground": "#6f6f6e"
      }
    },
    {
      "scope": "comment.line.double-dash.documentation.lua",
      "settings": {
        "foreground": "#40712d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "storage.type.annotation.lua",
      "settings": {
        "foreground": "#16f756",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.class.lua",
      "settings": {
        "foreground": "#2f5f9c"
      }
    },
    {
      "scope": "support.type.lua",
      "settings": {
        "foreground": "#4ab3d1"
      }
    },
    {
      "scope": "entity.name.variable.lua",
      "settings": {
        "foreground": "#4ab3d1"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#6a8f6b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.character.escape.lua",
      "settings": {
        "foreground": "#a37543",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.character.escape.byte.lua",
      "settings": {
        "foreground": "#3f6fc9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.character.escape.unicode.lua",
      "settings": {
        "foreground": "#2f7f8c",
        "fontStyle": "600"
      }
    },
    {
      "scope": "invalid.illegal.character.escape.lua",
      "settings": {
        "foreground": "#a83232",
        "background": "#f3dede",
        "fontStyle": "bold underline wavy"
      }
    },
    {
      "scope": "punctuation.definition.block.tag.ldoc",
      "settings": {
        "foreground": "#945e9c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.class.ldoc",
      "settings": {
        "foreground": "#3f6fc9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.quoted.single.lua",
      "settings": {
        "foreground": "#7fb078"
      }
    },
    {
      "scope": "string.quoted.double.lua",
      "settings": {
        "foreground": "#7fb078"
      }
    },
    {
      "scope": "string.quoted.other.multiline.lua",
      "settings": {
        "foreground": "#7fb078"
      }
    },
    {
      "scope": "punctuation.definition.string.begin.lua",
      "settings": {
        "foreground": "#389c9c"
      }
    },
    {
      "scope": "punctuation.definition.string.end.lua",
      "settings": {
        "foreground": "#389c9c"
      }
    },
    {
      "scope": "constant.character.escape.lua",
      "settings": {
        "foreground": "#945e9c"
      }
    }
  ]
}
